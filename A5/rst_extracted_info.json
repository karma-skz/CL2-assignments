      "antithesis": "multinuc",
      "same-unit": "multinuc"
    },
    "segments": [
      {
        "id": "1",
        "text": "As your floppy drive writes or reads,",
        "parent": "14",
        "relation": "Background",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "2",
        "text": "a Syncom diskette is working four ways",
        "parent": "14",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "3",
        "text": "to keep loose particles and dust from causing soft errors, dropouts.",
        "parent": "2",
        "relation": "Elaboration",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "4",
        "text": "Cleaning agents on the burnished surface of the Ectype coating actually remove build-up from the head,",
        "parent": "16",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "5",
        "text": "while lubricating it at the same time.",
        "parent": "4",
        "relation": "Temporal",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "6",
        "text": "A carbon additive drains away static electricity",
        "parent": "17",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "7",
        "text": "before it can attract dust or lint.",
        "parent": "6",
        "relation": "Temporal",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "8",
        "text": "Strong binders hold the signal-carrying oxides tightly within the coating.",
        "parent": "21",
        "relation": "Joint",
        "relation_type": "multinuc",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "9",
        "text": "And the non-woven jacket liner,",
        "parent": "18",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "10",
        "text": "more than just wiping the surface,",
        "parent": "9",
        "relation": "Background",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "11",
        "text": "provides thousands of tiny pockets",
        "parent": "19",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "12",
        "text": "to keep what it collects.",
        "parent": "11",
        "relation": "Elaboration",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      }
    ]
  },
