        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "3",
        "text": "carried a message, a refutation, of claims",
        "parent": "13",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "4",
        "text": "that the jobless could be employed",
        "parent": "12",
        "relation": "span",
        "relation_type": "span",
        "is_nucleus": true,
        "is_satellite": false
      },
      {
        "id": "5",
        "text": "if only they showed enough moxie.",
        "parent": "4",
        "relation": "Condition",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
        "id": "6",
        "text": "Every rule has exceptions,",
        "parent": "16",
        "relation": "Contrast",
        "relation_type": "rst",
        "is_nucleus": false,
        "is_satellite": true
      },
      {
